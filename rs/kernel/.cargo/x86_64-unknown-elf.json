{
    "arch": "x86_64",
    "cpu": "x86-64",
    "data-layout": "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-f80:128-n8:16:32:64-S128",
    "disable-redzone": true,
    "exe-suffix": ".elf",
    "executables": true,
    "linker": "ld.lld",
    "linker-flavor": "ld",
    "llvm-target": "x86_64-unknown-none-elf",
    "max-atomic-width": 64,
    "os": "none",
    "panic-strategy": "abort",
    "relro-level": "off",
    "pre-link-args": {
        "ld": [
            "--entry=kernel_main",
            "--image-base=0x100000",
            "--static",
            "-z", "separate-code"
        ]
    },
    "target-pointer-width": "64"
}